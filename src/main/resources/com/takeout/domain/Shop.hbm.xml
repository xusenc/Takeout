<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
	<hibernate-mapping package="com.takeout.domain">
		<class name="Shop" table="shop">
			<id name="shopId" type="java.lang.Long">
				<generator class="identity" />
			</id>
			<property name="sellerId" not-null="true" type="java.lang.Long" />
			<property name="sellerName" not-null="true" type="java.lang.String" />
			<property name="mobile" not-null="true" type="java.lang.String" />
			<property name="cityId" not-null="true" type="java.lang.Long" />
			<property name="cityName" not-null="true" type="java.lang.String" />
			<property name="regionId" type="java.lang.Long" />
			<property name="regionName" type="java.lang.String" />
			<property name="businessTime" type="java.lang.String" />
			<property name="fee" type="java.math.BigDecimal" />
			<property name="minMoney" type="java.math.BigDecimal" />
			<property name="discount" type="java.math.BigDecimal" />
			<property name="notice" not-null="true" type="java.lang.String" />
			<property name="latitude" not-null="true" type="java.math.BigDecimal" />
			<property name="longitude" not-null="true" type="java.math.BigDecimal" />
			<property name="address" type="java.lang.String" />
			<property name="linkman" type="java.lang.String" />
			<property name="linkPhone" type="java.lang.String" />
			<property name="lastUpdate" not-null="true" type="java.sql.Timestamp" />
			<property name="addTime" type="java.util.Date" />
			<property name="numClick" type="java.lang.Long" />
			<property name="numPraise" type="java.lang.Long" />
			<property name="isActivity" type="java.lang.Byte" />
			<property name="activityId" type="java.lang.Long" />
			<property name="activityName" type="java.lang.String" />
			<property name="announceId" type="java.lang.Long" />
			<property name="score" type="java.lang.Integer" />
			<property name="email" type="java.lang.String" />
		</class>
</hibernate-mapping>